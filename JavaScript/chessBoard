let s="";
let i=0;
let size=8;
while(i<8)
{
    for(j=0;j<4;j++)
    {
        if(i%2===0)
            s+=" " +"#";
        else
        {
            s+="#"+" ";
        }
    }
    s+="\n";
    i++;
}
console.log(s);
